{
  "name": "chat-quest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --base=/chat",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "vite",
    "build:docker": "docker build -t lyceum-cloud .",
    "start:docker": "docker run -p 4000:4000 lyceum-cloud"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "use-debounce": "^10.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
